{
  "cli": {
    "version": ">= 3.0.0"
  },
  "build": {
    "preview": {
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleRelease --warning-mode all",
        "buildType": "apk"
      },
      "node": "20.11.1",
      "cache": {
        "key": "v1"
      },
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      }
    },
    "production": {
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      },
      "node": "20.11.1",
      "cache": {
        "key": "v1"
      },
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      }
    }
  }
}
